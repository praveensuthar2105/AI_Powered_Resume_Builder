{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "http://ats-resify-env.eba-yhbmq4cr.ap-south-1.elasticbeanstalk.com/api/:path*"
        },
        {
            "source": "/oauth2/:path*",
            "destination": "http://ats-resify-env.eba-yhbmq4cr.ap-south-1.elasticbeanstalk.com/oauth2/:path*"
        },
        {
            "source": "/login/oauth2/:path*",
            "destination": "http://ats-resify-env.eba-yhbmq4cr.ap-south-1.elasticbeanstalk.com/login/oauth2/:path*"
        },
        {
            "source": "/auth/:path*",
            "destination": "http://ats-resify-env.eba-yhbmq4cr.ap-south-1.elasticbeanstalk.com/auth/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}